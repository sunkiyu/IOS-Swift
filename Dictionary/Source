import UIKit

//Collection Type
//Dictionary

//Key - value
//순서 없다.

var nameOfStreet = [String : Any]()

nameOfStreet["302ro"] = "1st Street"
nameOfStreet["303ro"] = "2st Street"
nameOfStreet["aaa"] = 3

nameOfStreet

//nil
nameOfStreet["test"]

//덮어쓰기
nameOfStreet["302ro"] = "1st Street2"

//Optional 선언을 안해도 nil 처리 가능
//var nameOfStreet = [String : String?]() //String?처리 안해도 nil 대입가능
nameOfStreet["aaa"] = nil
nameOfStreet
var nameOfStreet2 = ["a" : 1, "b" : 2]

//키 확인
nameOfStreet.keys

for dic in  nameOfStreet {
    print(dic)
}
